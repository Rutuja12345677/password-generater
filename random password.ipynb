    "import random\n",
    "import string\n",
    "\n",
    "\n",
    "def randompassword(passwordlen=10):\n",
    "    letter=string.ascii_uppercase\n",
    "    return ''.join(random.choice(letter) for i in range(passwordlen))\n",
    "print(\"random password :\\n\",randompassword())\n",
    "\n"
   ]
